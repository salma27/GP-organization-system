{
    "name": "backend",
    "version": "1.0.0",
    "type": "module",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "babel server -d dist --source-maps",
        "serve": "set NODE_ENV=production&&node dist/index.js",
        "dev": "set NODE_ENV=development&&babel-node server/index.js",
        "start": "set DEBUG=server:debug&&set NODE_ENV=development&&nodemon server/index.js --exec babel-node",
        "test": "set DEBUG=server:debug&&set NODE_ENV=test&&mocha --exit tests/",
        "test:watch": "set DEBUG=server:debug&&set NODE_ENV=development&&nodemon --exec mocha tests/**/*Test.js",
        "eslint": "eslint server/**/*.js --ignore-pattern \"node_modules/\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/node": "^7.12.6",
        "@babel/preset-env": "^7.12.1",
        "@babel/register": "^7.12.1",
        "body-parser": "^1.19.0",
        "chai": "^4.2.0",
        "config": "^3.3.2",
        "debug": "^4.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-jwt": "^6.0.0",
        "jsonwebtoken": "^8.5.1",
        "mocha": "^8.2.1",
        "mongoose": "^5.10.12",
        "nodemon": "^2.0.6",
        "should": "^13.2.3",
        "supertest": "^6.0.1"
    }
}
